{
  "name": "kiro-workspace",
  "version": "1.0.0",
  "description": "Kiro IDE Workspace with full stack development support",
  "private": true,
  "scripts": {
    "dev": "npm run build && node dist/index.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Tests not configured yet\"",
    "lint": "echo \"Linting not configured yet\"",
    "github:setup": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 setup",
    "github:sync": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 sync",
    "github:status": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 status"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "typescript": "^5.0.0",
    "prisma": "^5.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}