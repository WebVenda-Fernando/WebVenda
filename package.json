{
  "name": "kiro-workspace",
  "version": "1.0.0",
  "description": "Kiro IDE Workspace with full stack development support",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "build": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "dev": "ng serve --configuration development",
    "build:prod": "ng build --configuration production",
    "github:setup": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 setup",
    "github:sync": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 sync",
    "github:status": "powershell -ExecutionPolicy Bypass -File ./github-kiro.ps1 status"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.53.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.5.6",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.0",
    "typescript": "~5.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
